<h2>List of Peeps</h2>
<p>Study them before the game starts</p>
<%= link_to "Edit your profile", edit_your_user_path %>
<ul>
    <% @users.each do |user| %>
        <% if user.has_photo? %>
            <li>
                <%= link_to(image_tag(user.photo_url || "", :alt => user.name, :class => "index-photos"), user_path(user)) %>
            </li>
        <% end %>
    <% end %>
</ul>